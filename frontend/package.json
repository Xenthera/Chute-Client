{
  "name": "chute-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:ui": "tsc -p tsconfig.json",
    "dev:backend": "cd ../backend && go run ./ -ui 127.0.0.1:8787",
    "dev:tauri": "tauri dev",
    "dev": "npm run build:ui && concurrently -k -n backend,tauri \"npm run dev:backend\" \"npm run dev:tauri\"",
    "build": "npm run build:ui && tauri build"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.9",
    "concurrently": "^9.0.1",
    "typescript": "^5.7.2"
  }
}

